{
  "name": "pulsar-admin-client",
  "version": "1.0.10",
  "description": "Missing admin client for Apache Pulsar. Autogenerated from Swagger definitions.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run generate-client && npm run ts-to-js && ./monkeypatch.js",
    "ts-to-js": "rm -rf dist && tsc --project .",
    "generate-client": "rm -rf api && openapi -i ../swagger.json --name Client --useUnionTypes --client node -o ./api",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tealtools/node-pulsar-admin-client.git"
  },
  "keywords": [],
  "author": "visortelle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tealtools/node-pulsar-admin-client/issues"
  },
  "homepage": "https://github.com/tealtools/node-pulsar-admin-client#readme",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "openapi-typescript-codegen": "^0.23.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "node-fetch": "^3.2.4"
  }
}
